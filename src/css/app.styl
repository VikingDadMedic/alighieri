fieldset
  border none

.bg-dark-page
  background-color $dark-page

.text-caption
  text-transform uppercase

@media print
  .q-header,
  .q-drawer
    display none

  .q-page-container
    padding 0 !important

  h1
    page-break-before always

  hr+h1
    page-break-before none

  hr
    visibility hidden
    page-break-after always
